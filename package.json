{
  "name": "cs3",
  "version": "1.0.0",
  "description": "ConsistentS3",
  "main": "index.js",
  "scripts": {
    "lint": "xo",
    "lint:fix": "xo --fix .",
    "test": "ava",
    "test:watch": "ava --watch --verbose",
    "test:snapshot": "ava --update-snapshots"
  },
  "author": {
    "name": "Philip Hadviger",
    "email": "datfinesoul@phad.me",
    "url": "github.com/datfinesoul"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-crypto/client-node": "^1.0.2",
    "aws-sdk": "^2.610.0"
  },
  "engines": {
    "node": ">= 8"
  },
  "devDependencies": {
    "ava": "^3.0.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "xo": "^0.25.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "...": "..."
    }
  },
  "ava": {
    "watch": false
  },
  "xo": {
    "space": true,
    "rules": {
      "func-names": 0,
      "capitalized-comments": 0,
      "object-curly-spacing": [
        "error", "always", { "objectsInObjects": false }
      ]
    }
  },
  "lint-staged": {
    "*": "xo"
  }
}
